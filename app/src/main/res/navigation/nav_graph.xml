    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@+id/navigation_home">
    <fragment
        android:id="@+id/navigation_home"
        android:name="com.amg.slimadownnow.ui.home.HomeTabFragment"
        android:label="@string/title_home"
        tools:layout="@layout/frag_tab_home">

    </fragment>

    <fragment
        android:id="@+id/navigation_weight"
        android:name="com.amg.slimadownnow.ui.weight.tab.WeightTabFragment"
        android:label="@string/title_weight"
        tools:layout="@layout/frag_tab_weight">

    </fragment>

    <fragment
        android:id="@+id/navigation_food"
        android:name="com.amg.slimadownnow.ui.food.FoodTabFragment"
        android:label="@string/title_food"
        tools:layout="@layout/frag_tab_food">

    </fragment>

    <fragment
        android:id="@+id/navigation_exercise"
